{
  "name": "automation-seed",
  "version": "0.1.0",
  "description": "Automation framework using wdio",
  "private": true,
  "keywords": [
    "automation",
    "wdio",
    "selenium",
    "webdriver",
    "typescript"
  ],
  "moduleDirectories": [
    "src",
    "node_modules"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,sass,scss,less,html,md,yml,yaml}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "report": "allure generate",
    "selenoid": "./bin/selenoid",
    "test": "wdio run wdio.conf.js --no-autoCompileOpts.autoCompile",
    "test:local": "MODE=local yarn test",
    "test:remote": "MODE=remote yarn test",
    "suite:files": "ts-node ./bin/suite-spec-paths.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@rpii/wdio-commands": "^7.1.0",
    "@rpii/wdio-report-events": "^0.2.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@wdio/allure-reporter": "^7.30.2",
    "@wdio/cli": "^7.30.2",
    "@wdio/local-runner": "^7.30.2",
    "@wdio/mocha-framework": "^7.30.2",
    "@wdio/runner": "^7.30.2",
    "@wdio/selenium-standalone-service": "^7.30.2",
    "@wdio/spec-reporter": "^7.30.2",
    "allure-commandline": "^2.21.0",
    "axios": "^1.3.4",
    "chromedriver": "^119.0.1",
    "danger": "^11.2.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^9.3.2",
    "log4js": "^6.9.1",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2",
    "wdio-docker-service": "^3.2.1"
  },
  "overrides": {
    "decode-uri-component": "^0.2.1",
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.2",
    "minimatch": "^3.0.5",
    "ua-parser-js": "^1.0.33"
  }
}
